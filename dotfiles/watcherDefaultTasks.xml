<application>
  <component name="ApplicationTasksOptions">
    <global-watchers>
      <TaskOptions>
        <option name="arguments" value="fmt --manifest-path $ProjectFileDir$/Cargo.toml --all" />
        <option name="checkSyntaxErrors" value="true" />
        <option name="description" />
        <option name="exitCodeBehavior" value="ERROR" />
        <option name="fileExtension" value="rs" />
        <option name="immediateSync" value="true" />
        <option name="name" value="CargoFmt" />
        <option name="output" value="$ProjectFileDir$" />
        <option name="outputFilters">
          <array />
        </option>
        <option name="outputFromStdout" value="false" />
        <option name="program" value="$USER_HOME$/.cargo/bin/cargo" />
        <option name="runOnExternalChanges" value="true" />
        <option name="scopeName" value="Project Files" />
        <option name="trackOnlyRoot" value="false" />
        <option name="workingDir" value="$ProjectFileDir$" />
        <envs />
      </TaskOptions>
      <TaskOptions>
        <option name="arguments" value="-t py311 $FilePath$" />
        <option name="checkSyntaxErrors" value="true" />
        <option name="description" />
        <option name="exitCodeBehavior" value="ERROR" />
        <option name="fileExtension" value="py" />
        <option name="immediateSync" value="true" />
        <option name="name" value="Black" />
        <option name="output" value="$FilePath$" />
        <option name="outputFilters">
          <array />
        </option>
        <option name="outputFromStdout" value="false" />
        <option name="program" value="$USER_HOME$/.pyenv/shims/black" />
        <option name="runOnExternalChanges" value="true" />
        <option name="scopeName" value="Project Files" />
        <option name="trackOnlyRoot" value="false" />
        <option name="workingDir" value="$ProjectFileDir$" />
        <envs />
      </TaskOptions>
    </global-watchers>
  </component>
</application>
