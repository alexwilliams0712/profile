[user]
  email = alexwilliams0712@hotmail.com
  name = AlexWilliams
[core]
    autocrlf = true
    editor = sublime-text
[http]
    sslVerify = false
[diff]
    tool = bc3
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[push]
  default = simple
[merge]
  tool = kdiff3
[difftool]
  prompt = false
[alias]
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit
[url "ssh://git@github.com/"]
  insteadOf = https://github.com/
